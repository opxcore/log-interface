{
  "name": "opxcore/log-interface",
  "description": "The OpxCore logger interface.",
  "license": "MIT",
  "homepage": "https://opxcore.com",
  "type": "library",
  "support": {
    "source": "https://github.com/opxcore/log-interface",
    "issues": "https://github.com/opxcore/log-interface/issues"
  },
  "authors": [
    {
      "name": "Lozovoy Vyacheslav",
      "email": "opxcore@gmail.com"
    }
  ],
  "require": {
    "php": "^7.4",
    "psr/log": "^1.1"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5.0"
  },
  "autoload": {
    "psr-4": {
      "OpxCore\\Log\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "OpxCore\\Tests\\Log\\": "tests/"
    }
  },
  "prefer-stable": true,
  "minimum-stability": "dev"
}
